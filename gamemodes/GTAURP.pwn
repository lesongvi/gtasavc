SortWinnerPaintballScores(arenaid)
{
	new highscore = 0;
	new score = 0;
	new winnerid;
	for(new i = 0; i < PaintBallArena[arenaid][pbLimit]; i++)
	{
	    foreach(Player, p)
	    {
	        if(GetPVarInt(p, "IsInArena") == arenaid)
	        {
	            score = PlayerInfo[p][pKills];
	            if(score > highscore)
	            {
					highscore = score;
					winnerid = p;
	            }
	        }
	    }
	}
	return winnerid;
}

