public Lotto(number)
{
	new JackpotFallen = 0;
	new string[128];
	new winner[MAX_PLAYER_NAME];
	format(string, sizeof(string), "Tin tuc so xo: So may man hom nay la... %d!.", number);
	OOCOff(COLOR_WHITE, string);
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		if(PlayerInfo[i][pLottoNr] > 0)
		{
			if(PlayerInfo[i][pLottoNr] == number)
			{
				JackpotFallen = 1;
				GetPlayerName(i, winner, sizeof(winner));
				format(string, sizeof(string), "Tin tuc xo so: %s has won the jackpot of $10,000,000 with their lottery ticket.", winner, Jackpot);
				OOCOff(COLOR_WHITE, string);
				format(string, sizeof(string), "* You have won $10,000,000 with your lottery ticket - congratulations!", Jackpot);
				SendClientMessageEx(i, COLOR_YELLOW, string);
				GivePlayerCash(i, 10000000);
			}
			else
			{
				SendClientMessageEx(i, COLOR_LIGHTBLUE, "* You haven't won the lottery... better luck next time!");
			}
		}
		PlayerInfo[i][pLottoNr] = 0;
	}
	if(JackpotFallen)
	{
		new rand = random(125000); rand += 1;
		Jackpot = rand;
		SaveStuff();
		format(string, sizeof(string), "Tin tuc xo so: The new jackpot has been started.", Jackpot);
		OOCOff(COLOR_WHITE, string);
	}
	else
	{
		new rand = random(15000); rand += 2158;
		Jackpot += rand;
