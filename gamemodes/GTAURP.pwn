CMD:hammowithdraw(playerid, params[])
{
    new string[128], szString[128], ammotype[64], amount;
    new playername[MAX_PLAYER_NAME];
	if(GetPVarInt(playerid, "IsInArena") >= 0)
	{
		SendClientMessageEx(playerid, COLOR_WHITE, "You can't do this right now, you are in a arena!");
		return 1;
	}
	if(GetPVarInt( playerid, "EventToken") != 0)
	{
		SendClientMessageEx(playerid, COLOR_GREY, "You can't use this while you're in an event.");
		return 1;
	}
	if(GetPVarInt(playerid, "Injured") != 0||PlayerCuffed[playerid]!=0||GetPVarInt(playerid, "Hospital")!=0||GetPlayerState(playerid) == 7)
	{
		SendClientMessageEx (playerid, COLOR_GRAD2, "You cannot do this at this time.");
		return 1;
	}
	if(sscanf(params, "s[64]d", ammotype, amount))
	{
		SendClientMessageEx(playerid, COLOR_GRAD2, "USAGE: /hammowithdraw [ammotype] [amount]");
		SendClientMessageEx(playerid, COLOR_GRAD2, "Available Names: calammo, akammo, m4ammo, coltammo, shotgunammo, sniperammo, rocket");
		return 1;
	}
	new Float: Health;
	GetPlayerHealth(playerid, Health);
	if(Health < 80.0) return SendClientMessageEx(playerid,COLOR_GREY,"You cannot store ammo in a house when your health lower than 80.");
	if( GetPlayerVirtualWorld(playerid) == PlayerInfo[playerid][pPhousekey]+6000 )
	{
		if(PlayerInfo[playerid][pPhousekey] != INVALID_HOUSE_ID && strcmp(playername, HouseInfo[PlayerInfo[playerid][pPhousekey]][hOwner], true) == 0)
		{
			if(strcmp(ammotype, "calammo", true) == 0)
			{
				if (amount > PlayerInfo[playerid][pHouseCalAmmo] || amount < 1)
				{
					SendClientMessageEx(playerid, COLOR_GRAD2, "You don't have that much ammo!");
					return 1;
				}
