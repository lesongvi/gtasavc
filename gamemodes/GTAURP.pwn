CMD:mycardrugs(playerid, params[])
{
    	new szString[128];
    	new pvid = -1, Float: x, Float: y, Float: z;
    	for(new d = 0 ; d < MAX_PLAYERVEHICLES; d++)
		{
			if(PlayerVehicleInfo[playerid][d][pvId] != INVALID_PLAYER_VEHICLE_ID) GetVehiclePos(PlayerVehicleInfo[playerid][d][pvId], x, y, z);
			if(IsPlayerInRangeOfPoint(playerid, 3.0, x, y, z))
			{
				pvid = d;
				break;
			}
		}
		if(pvid == -1) return SendClientMessageEx(playerid,COLOR_GREY,"You are not near any vehicle that you own.");
		format(szString, sizeof(szString), "(Vehicle)Speed: {FFD700}%d", PlayerInfo[playerid][pCarSpeed]);
		SendClientMessageEx(playerid, COLOR_GRAD3, szString);
		format(szString, sizeof(szString), "(Vehicle)Crack: {FFD700}%d", PlayerInfo[playerid][pCarCrack]);
		SendClientMessageEx(playerid, COLOR_GRAD3, szString);
		format(szString, sizeof(szString), "(Vehicle)Pot: {FFD700}%d", PlayerInfo[playerid][pCarPot]);
		SendClientMessageEx(playerid, COLOR_GRAD3, szString);
		format(szString, sizeof(szString), "(Vehicle)Heroin: {FFD700}%d", PlayerInfo[playerid][pCarHeroin]);
		SendClientMessageEx(playerid, COLOR_GRAD3, szString);
		return 1;
}



