	new engine,lights,alarm,doors,bonnet,boot,objective,vehicleid;
		vehicleid = GetPlayerVehicleID(playerid);
		if(GetVehicleModel(vehicleid) == 481 || GetVehicleModel(vehicleid) == 509 || GetVehicleModel(vehicleid) == 510) return 1;
		if (IsVIPcar(vehicleid) || IsAdminSpawnedVehicle(vehicleid) || IsFamedVeh(vehicleid) || GetVehicleModel(vehicleid) == 481 || GetVehicleModel(vehicleid) == 509 || GetVehicleModel(vehicleid) == 510) return 1;
		GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
		if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) return 1;
		if(engine == VEHICLE_PARAMS_OFF) return 1;
		if(VehicleFuel[vehicleid] <= 0.0) return 1;
		VehicleFuel[vehicleid] -= 1.0;
	return 1;
}


