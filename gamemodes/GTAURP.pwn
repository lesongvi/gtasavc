    return 1;
}

CMD:buyguns(playerid, params[])
{
	for(new b; b < sizeof(Businesses); b++)
	{
    	if(GetPlayerVirtualWorld(playerid) == BUSINESS_BASE_VW + b)
		{
			if(Businesses[b][bBInventory] == 0)
			{
				SendClientMessageEx(playerid, COLOR_WHITE, "This store does not have any items at the moment!");
				return 1;
			}
			if(!IsAtGunShop(playerid))
			{
		        SendClientMessageEx(playerid, COLOR_GRAD2, "You are not at any Ammunation!");
		        return 1;
		    }
			if(PlayerInfo[playerid][pGunLic] != 1)
			{
				SendClientMessageEx(playerid,COLOR_GREY,"You don't have a weapon license!");
				return 1;
			}
		    ShowPlayerDialog(playerid, SHOPGUNMENU, DIALOG_STYLE_LIST, "Ammunation", "Remington Shotgun		$20,000\nGlock 10		$5,000\nKnife	$1,000\nVest		$10,000\nSilencer Attachment		$100,000\nTwo weapons		$500,000", "Buy", "Huy bo");
		    craftingammo[playerid] = 1;
		    Businesses[b][bBInventory] -= 1;
		    SaveBusiness();
		    return 1;
 		}
	}
	SendClientMessageEx(playerid, COLOR_GRAD1, "You must be in an active business!");
	return 1;
}

