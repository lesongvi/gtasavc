		else if(STDPlayer[j]==3)
		{
			GetPlayerHealth(j, health);
			SetPlayerHealth(j, health - 20.0);
			SendClientMessageEx(j, COLOR_LIGHTBLUE, "* Lost 12 health due to STD.");
		}
		if(GetPlayerCash(j) < 0)
		{
			if(MoneyMessage[j]==0)
			{
				new string[86];
				format(string, sizeof(string), "You're now in debt from borrowing the money from the bank. You must repay your debt.", GetPlayerCash(j));
