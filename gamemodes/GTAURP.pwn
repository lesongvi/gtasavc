CMD:redeemdrugs(playerid, params[])
{
    new szString[128];
	if(!IsPlayerInRangeOfPoint(playerid,20.0,2332.7141,-504.9596,5.9263))
	{
		SendClientMessage(playerid, COLOR_GRAD2, "You aren't at Vice Point's Pay and Spray garage!");
 		return 1;
	}
	if(PlayerInfo[playerid][pPack] == 0)
	{
		SendClientMessage(playerid, COLOR_GRAD2, "You don't have any drugs packages!");
 		return 1;
	}
	new string[128], amount;
	if(sscanf(params, "d", amount))
	{
		SendClientMessageEx(playerid, COLOR_WHITE, "USAGE: /redeemdrugs [amount]");
		format(string, sizeof(string), "You currently have %d drug packages.", PlayerInfo[playerid][pPack]);
		SendClientMessageEx(playerid, COLOR_GRAD3, string);
		return 1;
	}
	if (amount > PlayerInfo[playerid][pPack] || amount < 1)
	{
		SendClientMessageEx(playerid, COLOR_GRAD2, "   You don't have that much.");
		return 1;
	}
	format(szString, sizeof(szString), "You recieved {FFD700}%s packages of..", number_format(amount));
	SendClientMessageEx(playerid, COLOR_RED, szString);
	format(szString, sizeof(szString), "%s {FFD700}Speed in pill forms.", number_format(amount * 10));
	SendClientMessageEx(playerid, COLOR_WHITE, szString);
	format(szString, sizeof(szString), "%s {FFD700}Heroin in pill forms.", number_format(amount * 10));
	SendClientMessageEx(playerid, COLOR_WHITE, szString);
	format(szString, sizeof(szString), "%s {FFD700}Grams of Pot.", number_format(amount * 100));
	SendClientMessageEx(playerid, COLOR_WHITE, szString);
	format(szString, sizeof(szString), "%s {FFD700}Grams of Crack.", number_format(amount * 100));
	SendClientMessageEx(playerid, COLOR_WHITE, szString);
	PlayerInfo[playerid][pPot] += amount * 50;
	PlayerInfo[playerid][pCrack] += amount * 50;
	PlayerInfo[playerid][pHeroin] += amount * 10;
	PlayerInfo[playerid][pDSpeed] += amount * 10;
	PlayerInfo[playerid][pPack] -= amount;
	return 1;
}


