		{
			OnPlayerOfflineLogin(name);
			if( PlayerInfo[MAX_PLAYERS][pPermaBanned] == 3 || PlayerInfo[MAX_PLAYERS][pBanned] >= 1 )
			{
				SendClientMessageEx( playerid, COLOR_WHITE, "That player is banned." );
