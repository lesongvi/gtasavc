forward bleedtimer(playerid);
public bleedtimer(playerid)
{
    new string[128];
	    if(gPlayerLogged{playerid}) return 0;
	    if(PlayerInfo[playerid][pAdminDuty] == 1) return 1;
	    if(NoLife[playerid] == 1) return 1;
		new Float:health;
		GetPlayerHealth(playerid, health);
		if(PlayerInfo[playerid][pBleeding] == 5)
  		{
    		PlayerInfo[playerid][pHunger] = 50;
            SetPlayerHealth( playerid, 0 );
			SetPlayerSkillLevel(playerid, WEAPONSKILL_PISTOL, 1);
			SetPlayerSkillLevel(playerid, WEAPONSKILL_MICRO_UZI, 1);
			SetPlayerSkillLevel(playerid, WEAPONSKILL_SAWNOFF_SHOTGUN, 1);
			format(string, sizeof(string), "* %s bleeds and falls to the ground.", GetPlayerNameEx(playerid));
	  		ProxDetector(15.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
	  		PlayerInfo[playerid][pBleeding] = 0;
	  		SetPlayerDrunkLevel(playerid, 0);
  			return 1;
  		}
		if(health < 20)
		{
			format(string, sizeof(string), "* %s bleeds.", GetPlayerNameEx(playerid));
			ProxDetector(15.0, playerid, string, COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE,COLOR_PURPLE);
			PlayerInfo[playerid][pBleeding] += 1;
			SetPlayerDrunkLevel(playerid, 50000);
			return 1;
		}
		if(health > 20)
		{
			PlayerInfo[playerid][pBleeding] = 0;
			SetPlayerDrunkLevel(playerid, 0);
			return 1;
		}
	return 1;
}


