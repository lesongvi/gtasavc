                TogglePlayerControllable(playerid, 0);
                GameTextForPlayer(playerid, "Objects loading...", 4000, 5);
                SetPVarInt(playerid, "LoadingObjects", 1);
                SetTimerEx("SafeLoadObjects", 4000, 0, "d", playerid);
