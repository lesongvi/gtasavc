{
	if(!IsPlayerInRangeOfPoint(playerid,3.0,-1470.6781,1143.5992,40.0099))
	{
        SendClientMessageEx(playerid, COLOR_GRAD2, "You are not at the Shorside Bridge Lift control panel!");
        return 1;
    }
