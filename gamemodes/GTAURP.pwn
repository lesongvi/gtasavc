LoadFlags()
{
	new arrCoords[6][64];
	new strFromFile2[256];
	new File: file = fopen("flags.cfg", io_read);
	if(file)
	{
		new idx;
		while (idx < sizeof(PlayerFlag))
		{
			fread(file, strFromFile2);
			splits(strFromFile2, arrCoords, '|');

            PlayerFlag[idx][pCreated] = strval(arrCoords[0]);

			strmid(PlayerFlag[idx][pFlagName], arrCoords[1], 0, strlen(arrCoords[1]), 128);
			strmid(PlayerFlag[idx][pFlagDescription], arrCoords[2], 0, strlen(arrCoords[2]), 128);
			strmid(PlayerFlag[idx][pAssignedFlagTo], arrCoords[3], 0, strlen(arrCoords[3]), MAX_PLAYER_NAME);
			strmid(PlayerFlag[idx][pAssignedFlagBy], arrCoords[4], 0, strlen(arrCoords[4]), MAX_PLAYER_NAME);
