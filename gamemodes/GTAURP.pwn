                PlayerPlayMusic(playerid);
                SendClientMessageEx(playerid, COLOR_WHITE, "Congratulations, On Your New Purchase.");
                SendClientMessageEx(playerid, COLOR_WHITE, "Type /help to review the new car dealership help section.");
