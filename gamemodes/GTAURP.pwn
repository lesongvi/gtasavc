CMD:buyfood(playerid, params[])
{
	for(new b; b < sizeof(Businesses); b++)
	{
    	if(GetPlayerVirtualWorld(playerid) == BUSINESS_BASE_VW + b)
		{
			if(Businesses[b][bBInventory] == 0)
			{
				SendClientMessageEx(playerid, COLOR_WHITE, "This store does not have any items at the moment!");
				return 1;
			}
		 	if(!IsAtFood(playerid))
			{
			    SendClientMessageEx(playerid, COLOR_GRAD2, "You aren't at any food place.");
			    return 1;
			}
			if(GetPlayerCash(playerid) < 8000)
			{
		 		SendClientMessageEx(playerid, COLOR_GRAD2, "You don't have $8,000 for food.");
		 		return 1;
			}
		 	GivePlayerCash(playerid, -8000);
			SendClientMessageEx(playerid, COLOR_GRAD2, "You brought some fresh food! /myfood!");
			PlayerInfo[playerid][pFreshFood] += 1;
			Businesses[b][bBInventory] -= 1;
			Businesses[b][bSafeBalance] += 8000;
			SaveBusiness();
			return 1;
		}
	}
	SendClientMessageEx(playerid, COLOR_GRAD1, "You must be in an active business!");
	return 1;
}



