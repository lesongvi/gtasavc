CMD:clothes(playerid, params[])
{
	if(IsAtClothShop(playerid))
	{
		for(new b; b < sizeof(Businesses); b++)
		{
	    	if(GetPlayerVirtualWorld(playerid) == BUSINESS_BASE_VW + b || GetPlayerVirtualWorld(playerid) == 559 )
			{
			    if(PlayerInfo[playerid][pKickTut] == 1)
				{
					if(Businesses[b][bBInventory] == 0)
					{
						SendClientMessageEx(playerid, COLOR_WHITE, "This store does not have any items at the moment!");
						return 1;
					}
				}
				if(GetPlayerSkin(playerid) == 138 || GetPlayerSkin(playerid) == 252)
				{
			 		if(GetPlayerCash(playerid) < 2500) return SendClientMessageEx(playerid, COLOR_GRAD2, "You can't afford these clothes!  ($2,500)");
					TogglePlayerControllable(playerid, 0);
					SelectChar[playerid] = 1;
					SelectCharPlace[playerid] = 1;
					PlayerInfo[playerid][pModel] = ChosenSkin[playerid];
					PlayerInfo[playerid][pChar] = ChosenSkin[playerid];
					SendClientMessageEx(playerid, COLOR_GRAD2, "* Press Y to select the clothes you want to use.");
					SendClientMessageEx(playerid, COLOR_GRAD2, "* If you've found the clothes you want to use, press SHIFT.");
					if(PlayerInfo[playerid][pKickTut] == 1)
					{
    					Businesses[b][bBInventory] -= 1;
    					Businesses[b][bSafeBalance] += 2500;
    					SaveBusiness();
 					}
					buyingclothes[playerid] = 1;
					return 1;
				}
				SendClientMessageEx(playerid, COLOR_GRAD1, "You must remove your clothes to try new ones on! /removeclothes");
				return 1;
			}
		}
		SendClientMessageEx(playerid, COLOR_GRAD1, "You must be in an active business!");
		return 1;
	}
	else
	{
		SendClientMessageEx( playerid, COLOR_WHITE, "You are not in a Clothing Shop!" );
		return 1;
	}
}


