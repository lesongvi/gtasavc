CMD:placeyacht(playerid, params[])
{
	if(PlayerInfo[playerid][pYacht] == 0)
	{
		SendClientMessage(playerid, COLOR_GRAD1, "You don't own a Yacht!");
		return 1;
	}
    if(!IsPlayerInWater(playerid)) return SendClientMessage(playerid, COLOR_GRAD1, "You must be in the ocean to spawn the Yacht!.");
    new string[128], Float:YachtCoord[4], pName[MAX_PLAYER_NAME];
    GetPlayerPos(playerid, YachtCoord[0], YachtCoord[1], YachtCoord[2]);
    GetPlayerFacingAngle(playerid, YachtCoord[3]);
    SetPVarFloat(playerid, "YachtX", YachtCoord[0]);
    SetPVarFloat(playerid, "YachtY", YachtCoord[1]);
    SetPVarFloat(playerid, "YachtZ", YachtCoord[2]);
    GetPlayerName(playerid, pName, sizeof(pName));
    YachtCoord[0] += (2 * floatsin(-YachtCoord[3], degrees));
   	YachtCoord[1] += (2 * floatcos(-YachtCoord[3], degrees));
   	YachtCoord[2] -= 1.0;
	if(GetPVarInt(playerid, "PlacedYacht")) return SendClientMessageEx(playerid, COLOR_GRAD1, "You already have a yacht out!");
	foreach(Player, i)
	{
 		if(GetPVarType(i, "PlacedYacht"))
   		{
  			if(IsPlayerInRangeOfPoint(playerid, 30.0, GetPVarFloat(i, "YachtX"), GetPVarFloat(i, "YachtY"), GetPVarFloat(i, "YachtZ")))
			{
   				SendClientMessage(playerid, COLOR_WHITE, "You cannot put your yacht in this Radius as their is already one placed in this radius");
			    return 1;
			}
		}
	}
	SetPVarInt(playerid, "PlacedYacht", CreateDynamicObject(10565, YachtCoord[0], YachtCoord[1]+32, YachtCoord[2]+6, 0.0, 0.0, 0.0, .worldid = GetPlayerVirtualWorld(playerid), .interiorid = GetPlayerInterior(playerid)));
	format(string, sizeof(string), "Yacht Owner: %s", pName);
	SetPVarInt(playerid, "YachtLabel", _:CreateDynamic3DTextLabel(string, -1, YachtCoord[0], YachtCoord[1]+32, YachtCoord[2]+5.6, 5, .worldid = GetPlayerVirtualWorld(playerid), .interiorid = GetPlayerInterior(playerid)));
	SetPVarInt(playerid, "YachtArea", CreateDynamicSphere(YachtCoord[0], YachtCoord[1], YachtCoord[2], 30.0, GetPlayerVirtualWorld(playerid), GetPlayerInterior(playerid)));
	SetPVarInt(playerid, "YachtInt", GetPlayerInterior(playerid));
	SetPVarInt(playerid, "YachtVW", GetPlayerVirtualWorld(playerid));
	return 1;
}


