CMD:eleap(playerid, params[])
{
	new Float:x, Float:y, Float:z;
	GetPlayerPos(playerid, x, y, z);
	if(!IsPlayerInRangeOfPoint(playerid,10.0,2268.4927,-1372.2661, z))
	{
        SendClientMessageEx(playerid, COLOR_GRAD2, "You are not at the Washington Beach Appartments Elevator!");
        return 1;
    }
    if(IsDynamicObjectMoving(caselev)) return SendClientMessageEx(playerid, COLOR_GRAD2, "Someone is using this elevator!");
    ShowPlayerDialog(playerid, ELEMENU, DIALOG_STYLE_LIST, "Pick a floor.", "Ground Floor\nfloor 2\nfloor 3\nfloor 4\nfloor 5\nfloor 6\nfloor 7\nfloor 8\nfloor 9\nfloor 10\nfloor 11", "enter", "Huy bo");
    return 1;
}


