CMD:buyjacket(playerid, params[])
{
	for(new b; b < sizeof(Businesses); b++)
	{
	    if(GetPlayerVirtualWorld(playerid) == BUSINESS_BASE_VW + b || GetPlayerVirtualWorld(playerid) == 559 )
		{
		    if(PlayerInfo[playerid][pKickTut] == 1)
			{
				if(Businesses[b][bBInventory] == 0)
				{
					SendClientMessageEx(playerid, COLOR_WHITE, "This store does not have any items at the moment!");
					return 1;
				}
			}
		 	if(!IsAtClothShop(playerid))
			{
		        SendClientMessageEx(playerid, COLOR_GRAD2, "You aren't at a clothing shop!");
		        return 1;
		    }
		    if(PlayerInfo[playerid][pJacket] == 1)
			{
				SendClientMessage(playerid, COLOR_WHITE, "You already have a jacket!");
				return 1;
			}
		    if(GetPlayerCash(playerid) < 9999)
			{
				SendClientMessageEx(playerid, COLOR_GRAD1, "You don't have enough to get a jacket! ($10,000)");
				return 1;
			}
			GivePlayerCash(playerid, -10000);
			PlayerInfo[playerid][pJacket] = 1;
		    SendClientMessage(playerid, COLOR_GRAD2, "You brought a jacket!");
   			if(PlayerInfo[playerid][pKickTut] == 1)
			{
  				Businesses[b][bBInventory] -= 1;
  				Businesses[b][bSafeBalance] += 10000;
  				SaveBusiness();
			}
			return 1;
		}
	}
	SendClientMessageEx(playerid, COLOR_GRAD1, "You must be in an active business!");
	return 1;
}

