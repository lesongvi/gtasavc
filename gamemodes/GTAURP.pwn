SaveEventPoints()
{
	new idx;
	new File: file2;
	while (idx < sizeof(EventPoints))
	{
		new coordsstringf[326];
		format(coordsstringf, sizeof(coordsstringf), "%f|%f|%f|%d|%d|%s|%d\n",
			EventPoints[idx][epPosX],
			EventPoints[idx][epPosY],
			EventPoints[idx][epPosZ],
			EventPoints[idx][epVW],
			EventPoints[idx][epInt],
			EventPoints[idx][epPrize],
			EventPoints[idx][epFlagable]
		);
		if(idx == 0)
		{
			file2 = fopen("eventpoints.cfg", io_write);
		}
		else
		{
			file2 = fopen("eventpoints.cfg", io_append);
		}
		fwrite(file2, coordsstringf);
		idx++;
		fclose(file2);
	}
	idx = 0;

	return 1;
}

