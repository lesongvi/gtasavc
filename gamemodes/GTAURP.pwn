		}
		else if(STDPlayer[j]==2)
		{
			GetPlayerHealth(j, health);
			SetPlayerHealth(j, health - 12.0);
			SendClientMessageEx(j, COLOR_LIGHTBLUE, "* Lost 8 health due to STD.");
