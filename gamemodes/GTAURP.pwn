		else if (PlayerInfo[playerid][pTaxiLicense] == 1)
		{
			if(GetPlayerState(playerid) == 2)
			{
				if(fare < 1 || fare > 500)
				{
					SendClientMessageEx(playerid, COLOR_GREY, "   Fare price must be between $1 and $500!");
