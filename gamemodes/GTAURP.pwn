    return 1;
}


CMD:fps(playerid, params[])
{
	if(FPS[playerid] == false)
	{
		FPS[playerid] = true;
		StartFPS(playerid);
	}
	else if(FPS[playerid] == true)
	{
		FPS[playerid] = false;
		StopFPS(playerid);
	}
	return 1;
}


CMD:glocksl(playerid, params[])
{
    if(PlayerInfo[playerid][pSilencer] == 0) return SendClientMessageEx(playerid, COLOR_GRAD2, "You don't have this attachment!");
	if(GetPlayerWeapon(playerid) == 22)
	{
	    PlayerInfo[playerid][pSil] = 1;
	    PlayerInfo[playerid][pColt] = 0;
		SetTimerEx("craftammo", 1000, false, "d", playerid);
 		SendClientMessageEx(playerid, COLOR_GRAD2, "You have added an attachment to your weapon.");
	}
	else if(GetPlayerWeapon(playerid) == 23)
	{
	    PlayerInfo[playerid][pSil] = 0;
	    PlayerInfo[playerid][pColt] = 1;
		SetTimerEx("craftammo", 1000, false, "d", playerid);
 		SendClientMessageEx(playerid, COLOR_GRAD2, "You have taken off the attachment of your weapon.");
	}
