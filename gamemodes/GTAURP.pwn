CMD:buytoys(playerid, params[])
{
	for(new b; b < sizeof(Businesses); b++)
	{
	    if(GetPlayerVirtualWorld(playerid) == BUSINESS_BASE_VW + b || GetPlayerVirtualWorld(playerid) == 559 )
		{
		    if(PlayerInfo[playerid][pKickTut] == 1)
			{
				if(Businesses[b][bBInventory] == 0)
				{
					SendClientMessageEx(playerid, COLOR_WHITE, "This store does not have any items at the moment!");
					return 1;
				}
			}
			if(IsAtClothShop(playerid))
			{
				if(PlayerInfo[playerid][pKickTut] == 1)
				{
			    	Businesses[b][bBInventory] -= 1;
			    	Businesses[b][bSafeBalance] += 500;
			    	SaveBusiness();
   				}
				ShowPlayerDialog( playerid, BUYTOYS, DIALOG_STYLE_MSGBOX, "Toy Store", "Welcome to the toy store! Here you can buy accessories to attach to your player.\n\nFirst, we will choose a slot to store the toy in.\n(Note: Non-VIP can only hold 2 toys, Bronze-Gold VIP can hold 3-5)","Continue", "Huy bo" );
			}
			else
			{
				SendClientMessageEx( playerid, COLOR_WHITE, "   You are not in a Clothing Shop!" );
			}
			return 1;
		}
	}
	SendClientMessageEx(playerid, COLOR_GRAD1, "You must be in an active business!");
	return 1;
}

