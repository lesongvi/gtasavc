public AFKick(playerid)
{
	if(playerTabbed[playerid] != 0)
	{
 		playerTabbedTime[playerid]++;
 		SetTimerEx("AFKick",1000,false,"d",playerid);
	}
	return 1;
}
public OnPlayerUpdate(playerid)
{
    if(playerTabbed[playerid] >= 1)
	{
		playerTabbed[playerid] = 0;
	}
	if(PlayerInfo[playerid][pEnergy] == 0)
	{
	    if(IsPlayerNPC(playerid)) return 1;
	    if(PlayerInfo[playerid][pMuscles] > 40) return 1;
 		TogglePlayerControllable(playerid,0);
 		ApplyAnimation(playerid, "FAT", "IDLE_tired" , 4.1, 1, 1, 1, 1, 1, 1);
 		if(imtired[playerid] == 0)
   		{
			SetTimerEx("UnfreezeMyAss", 3000, false, "d", playerid);
		}
	}
	new Float:health;
    GetPlayerHealth(playerid, health);
    if(health > 100)
	{
    	if(PlayerInfo[playerid][pAdminDuty] == 1) return 1;
    	SetPlayerHealth(playerid, 100);
